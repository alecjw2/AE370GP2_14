inspace(-6, 6, 800)\n","X, Y = np.meshgrid(x, y)\n","Z = X + 1j * Y\n","\n","# Crank–Nicolson stability function\n","R = (1 + Z / 2) / (1 - Z / 2)\n","abs_R = np.abs(R)\n","\n","# Safely handle any infinities or NaNs\n","abs_R = np.nan_to_num(abs_R, nan=np.inf, posinf=np.inf, neginf=np.inf)\n","\n","# Plotting the stability region\n","plt.figure(figsize=(6, 6))\n","plt.contour(X, Y, abs_R, levels=[1], colors='black')\n","plt.contourf(X, Y, abs_R, levels=[0, 1], colors=['#A0CBE2'])  # fill inside region\n","plt.title(\"Crank–Nicolson Stability Region\")\n","plt.xlabel(\"Re(z)\")\n","plt.ylabel(\"Im(z)\")\n","plt.xlim([-6, 6])  # Now symmetrically centered around 0\n","plt.ylim([-6, 6])  # y-axis range remains the same\n","plt.gca().set_aspect('equal')\n","plt.grid(True)\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":607},"id":"re77RXqTjggM","executionInfo":{"status":"ok","timestamp":1746718891391,"user_tz":300,"elapsed":338,"user":{"displayName":"Alec Wojcik","userId":"13600781879686117157"}},"outputId":"2c7ab81f-88ab-44a8-cc16-19504ee04558"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 600x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":[],"metadata":{"id":"DlsItiakjwh6"},"execution_count":null,"outputs":[]}]}